<% if (errors.length > 0) { %>
<div class="row">
    <div class="col">
        <% errors.forEach(erro => { %>
      <div class="alert alert-danger"><%= erro %></div>
      <% }); %>
    </div>
  </div>
  <% } %>
  

  <% if (success.length > 0) { %>
    <div class="row">
        <div class="col">
            <% success.forEach(sucesso => { %>
          <div class="alert alert-success"><%= sucesso %></div>
          <% }); %>
        </div>
      </div>
      <% } %>